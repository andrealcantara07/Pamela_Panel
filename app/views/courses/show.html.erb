<h3>Courses</h3>




<%= link_to 'Log Out', logout_path, method: :delete, class: "waves-effect waves-light btn-small center-align"%>

<%= link_to 'Back', user_path, class: "waves-effect waves-light btn-small center-align"%>

<%= link_to 'Add New', new_course_path, class: "waves-effect waves-light btn-small center-align"%>


<% @courses.each do |course| %>
    <p><%= course.title %></p>
    <p><%= course.hours %></p>
    <p><%= course.description %></p>
    <%end%>